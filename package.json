{
    "name": "rs-react-native-paper-tabs",
    "version": "1.0.1",
    "description": "Smooth and fast cross platform Material Design Tabs for React Native Paper",
    "license": "MIT",
    "author": "rsoubrane",
    "type": "commonjs",
    "main": "./lib/index.js",
    "module": "lib/index.js",
    "types": "lib/index.d.ts",
    "directories": {
        "lib": "lib"
    },
    "files": [
        "src",
        "lib"
    ],
    "homepage": "https://github.com/web-ridge/react-native-paper-tabs#readme",
    "bugs": {
